
@{
    ViewData["Title"] = "MemberNutritionistList";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<!--**********************************
           Content body start
       ***********************************-->
<div class="content-body">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Danışan Kilo Geçmişi</h4>
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered zero-configuration">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Danışan</th>
                                        <th>Doğum Tarihi</th>
                                        <th>Meslek</th>   
                                        <th>Randevu Tarihi</th>
                                        <th>Boy</th>                                        
                                        <th>Kilo</th>                                        
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.Id</td>
                                            <td>@item.MemberNameSurname</td>
                                            <td>@item.Birthdate</td>
                                            <td>@item.Job</td>
                                            <td>@item.AppointmentDate</td>
                                            <td>@item.Size</td>
                                            <td>@item.Weight</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- #/ container -->
</div>
<!--**********************************
    Content body end
***********************************-->
